{
  "name": "@directus/app",
  "private": true,
  "version": "7.1.0",
  "description": "Directus is an Open-Source Headless CMS & API for Managing Custom Databases",
  "main": "src/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "fix:eslint": "eslint --fix",
    "fix:stylelint": "stylelint --fix",
    "fix:markdownlint": "markdownlint",
    "fix:prettier": "prettier --write --loglevel warn",
    "fix:all:eslint": "npm run fix:eslint --ext .js,.vue .",
    "fix:all:stylelint": "npm run fix:stylelint \"src/**/*.{vue,scss}\"",
    "fix:all:markdownlint": "npm run fix:markdownlint \"docs/*.md\" \"readme.md\"",
    "fix:all:prettier": "npm run fix:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "fix": "run-s fix:all:*"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "repository": "directus/app",
  "author": "Rijk van Zanten <rijkvanzanten@me.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/directus/app/issues"
  },
  "homepage": "https://github.com/directus/app#readme",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "cross-env": "^5.2.0",
    "eslint-plugin-vue": "^5.1.0",
    "ignore-loader": "^0.1.2",
    "imagemin-lint-staged": "^0.4.0",
    "lint-staged": "^8.1.0",
    "markdownlint-cli": "^0.13.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recess-order": "^2.0.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.1",
    "vue-template-compiler": "^2.5.22",
    "yorkie": "^2.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ],
  "dependencies": {
    "@directus/format-title": "^2.2.1",
    "@directus/sdk-js": "^5.1.4",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "vue": "^2.5.22",
    "vue-i18n": "^8.7.0",
    "vue-meta": "^1.5.8",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  }
}
